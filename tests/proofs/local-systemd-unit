Service file:
[Unit]
Description=Simple wallpaper changer
After=network.target

[Service]
Type=simple
Environment=DISPLAY=:0
ExecStart=python -m chwall.client next savetime
WorkingDirectory={path}

[Install]
WantedBy=default.target

Timer file:
[Unit]
Description=Change wallpaper every 10 minutes
After=network.target

[Timer]
OnCalendar=*:0/10

[Install]
WantedBy=timers.target
